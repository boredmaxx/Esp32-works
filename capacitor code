const int capPin = 32; 
int value = 0;         

void setup() {
  Serial.begin(115200); 
  delay(1000);          
  Serial.println("Capacitor Timer Test Started");
}

void loop() {
 value = analogRead(capPin);
  

  Serial.print("Capacitor Voltage Reading: ");
  Serial.println(value);
  

  delay(200);
